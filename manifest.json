{
    "name": "Galette",
    "id": "galette",
    "description": {
        "en": "Membership management web application for non profit organizations",
        "fr": "Outil de gestion d'adh√©rents et de cotisation en ligne pour associations"
    },
    "licence": "free",
    "maintainer": {
        "name": "yolateng0",
        "email": "yolateng0@yo.nohost.me",
        "url": "https://github.com/yolateng0/"
    },
    "multi_instance": "false",
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for galette"
                    "fr": "Choisir votre domane pour galette"
                },
                "example": "example.com"
            },
            {
                "name": "path",
                "ask": {
                    "en": "Choose a path for galette"
                },
                "example": "/galette",
                "default": "/galette"
            },
            {
                "name": "admin",
                "ask": {
                    "en": "Admin login"
                },
                "example": "galette_admin",
                "default": "galette_admin"
            },
            {
                "name": "adminPass",
                "ask": {
                    "en": "Admin password"
                },
                "example": ""
            },
            {
                "name": "is_public",
                "ask": {
                    "en": "Is it a public application ?"
                },
                "choices": ["Yes", "No"],
                "default": "Yes"
            }
        ]
    }
}
